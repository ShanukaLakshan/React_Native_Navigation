{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "jsx": "react-native",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "noImplicitAny": true
  },
  "include": ["src", "node_modules/@types"],
  "exclude": ["node_modules"]
}
